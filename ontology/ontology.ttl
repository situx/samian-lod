#################################################################
#    Datatypes
#################################################################

###  http://purl.org/dc/dcmitype/Image
dcmitype:Image rdf:type rdfs:Datatype ;
               rdfs:comment "Examples include images and photographs of physical objects, paintings, prints, drawings, other images and graphics, animations and moving pictures, film, diagrams, maps, musical notation. Note that Image may include both electronic and physical representations."@en ;
               rdfs:label "Image"@en .


###  http://www.w3.org/2000/01/rdf-schema#Resource
rdfs:Resource rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://archaeology.link/ontology#PlaceOfInterest
ontology:PlaceOfInterest rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf geosparql:hasGeometry ;
                         rdfs:comment "c.f. https://www.ogc.org/standards/geosparql, 1.0, OGC GeoSPARQL - A Geographic Query Language for RDF Data, 11-052r4, Annex B, B.1"@en ;
                         rdfs:label "place of interest"@en .


###  http://archaeology.link/ontology#carries
ontology:carries rdf:type owl:ObjectProperty ;
                 rdfs:domain ontology:InformationCarrier ;
                 rdfs:range ontology:Inscription ;
                 rdfs:label "carries"@en ,
                            "trägt"@de .


###  http://archaeology.link/ontology#centroid
ontology:centroid rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf ontology:hasPointGeometry ;
                  rdfs:domain geosparql:Geometry ;
                  rdfs:range owl:Thing ;
                  rdfs:label "has centroid"@en ,
                             "hat Zentrum"@de .


###  http://archaeology.link/ontology#clusteredArea
ontology:clusteredArea rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf ontology:hasExactGeometry ;
                       rdfs:domain geosparql:Geometry ;
                       rdfs:range owl:Thing ;
                       rdfs:label "has clustered area"@en ,
                                  "hat geclusterte Fläche"@de .


###  http://archaeology.link/ontology#clusteredAs
ontology:clusteredAs rdf:type owl:ObjectProperty ;
                     rdfs:domain ontology:Location ;
                     rdfs:range ontology:KilnRegion ;
                     rdfs:label "clustered as"@en ,
                                "geclustert als"@de .


###  http://archaeology.link/ontology#disclosedAt
ontology:disclosedAt rdf:type owl:ObjectProperty ;
                     rdfs:domain ontology:InformationCarrier ;
                     rdfs:range ontology:Place ;
                     rdfs:label "disclosed at"@en ,
                                "offenbart bei"@de .


###  http://archaeology.link/ontology#exactMatch
ontology:exactMatch rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf <http://www.wikidata.org/prop/direct/P2888> ;
                    rdf:type owl:SymmetricProperty ,
                             owl:TransitiveProperty ;
                    rdfs:domain owl:Thing ;
                    rdfs:range owl:Thing ;
                    rdfs:label "exact match"@en ,
                               "exakte Übereinstimmung"@de .


###  http://archaeology.link/ontology#generalisedAs
ontology:generalisedAs rdf:type owl:ObjectProperty ;
                       rdfs:domain ontology:Potform ;
                       rdfs:range ontology:GenericPotform ;
                       rdfs:label "generalised as"@en ,
                                  "generalisiert als"@de .


###  http://archaeology.link/ontology#hasAMT
ontology:hasAMT rdf:type owl:ObjectProperty ;
                rdfs:domain owl:Thing ;
                rdfs:range owl:Thing ;
                rdfs:label "has AMT node"@en ,
                           "hat AMT Knoten"@de .


###  http://archaeology.link/ontology#hasEntity
ontology:hasEntity rdf:type owl:ObjectProperty ;
                   rdfs:domain ontology:Actor ;
                   rdfs:range ontology:ActorEntity ;
                   rdfs:label "has entity"@en ,
                              "hat Entität"@de .


###  http://archaeology.link/ontology#hasExactGeometry
ontology:hasExactGeometry rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf geosparql:hasGeometry ;
                          rdfs:domain geosparql:Geometry ;
                          rdfs:range owl:Thing ;
                          rdfs:comment "c.f. https://www.ogc.org/standards/geosparql, 1.0, OGC GeoSPARQL - A Geographic Query Language for RDF Data, 11-052r4, Annex B, B.1"@en ;
                          rdfs:label "has exact geometry"@en .


###  http://archaeology.link/ontology#hasKilnsite
ontology:hasKilnsite rdf:type owl:ObjectProperty ;
                     owl:inverseOf ontology:isKilnsiteOf ;
                     rdfs:domain ontology:InformationCarrier ;
                     rdfs:range ontology:Location .


###  http://archaeology.link/ontology#hasLocation
ontology:hasLocation rdf:type owl:ObjectProperty ;
                     rdfs:domain ontology:Place ;
                     rdfs:range ontology:Location ;
                     rdfs:label "has location"@en ,
                                "hat Standort"@de .


###  http://archaeology.link/ontology#hasPointGeometry
ontology:hasPointGeometry rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf geosparql:hasGeometry ;
                          rdfs:domain geosparql:Geometry ;
                          rdfs:range owl:Thing ;
                          rdfs:comment "c.f. https://www.ogc.org/standards/geosparql, 1.0, OGC GeoSPARQL - A Geographic Query Language for RDF Data, 11-052r4, Annex B, B.1"@en ;
                          rdfs:label "has point geometry"@en .


###  http://archaeology.link/ontology#hasStatus
ontology:hasStatus rdf:type owl:ObjectProperty ;
                   rdfs:domain ontology:ActorEntity ;
                   rdfs:range ontology:ActorEntity ;
                   rdfs:label "has status"@en ,
                              "hat Status"@de .


###  http://archaeology.link/ontology#hasType
ontology:hasType rdf:type owl:ObjectProperty ;
                 rdfs:domain owl:Thing ;
                 rdfs:range crm:E55_Type ;
                 rdfs:label "has type"@en ,
