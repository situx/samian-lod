geosparql:hasGeometry rdf:type owl:ObjectProperty ;
                      rdfs:domain geosparql:SpatialObject ;
                      rdfs:range owl:Thing ;
                      rdfs:label "has geometry"@en ,
                                 "hat Geometrie"@de .


###  http://www.w3.org/2002/07/owl#sameAs
owl:sameAs rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf skos:exactMatch ;
           rdf:type owl:SymmetricProperty ,
                    owl:TransitiveProperty ;
           rdfs:domain owl:Thing ;
           rdfs:range owl:Thing ;
           dc:description "The property that determines that two given individuals are equal."@en ;
           rdfs:label "sameAs"@en .


###  http://www.w3.org/2004/02/skos/core#exactMatch
skos:exactMatch rdf:type owl:ObjectProperty ,
                         owl:SymmetricProperty ,
                         owl:TransitiveProperty ;
                rdfs:domain owl:Thing ;
                rdfs:range owl:Thing ;
                rdfs:label "has exact match"@en .


###  http://www.w3.org/ns/prov#actedOnBehalfOf
prov:actedOnBehalfOf rdf:type owl:ObjectProperty .


###  http://www.w3.org/ns/prov#endedAtTime
prov:endedAtTime rdf:type owl:ObjectProperty .


###  http://www.w3.org/ns/prov#startedAtTime
prov:startedAtTime rdf:type owl:ObjectProperty .


###  http://www.w3.org/ns/prov#used
prov:used rdf:type owl:ObjectProperty .


###  http://www.w3.org/ns/prov#wasAssociatedWith
prov:wasAssociatedWith rdf:type owl:ObjectProperty .


###  http://www.w3.org/ns/prov#wasAttributedTo
prov:wasAttributedTo rdf:type owl:ObjectProperty .


###  http://www.w3.org/ns/prov#wasDerivedFrom
prov:wasDerivedFrom rdf:type owl:ObjectProperty .


###  http://www.w3.org/ns/prov#wasGeneratedBy
prov:wasGeneratedBy rdf:type owl:ObjectProperty .


###  http://www.w3.org/ns/prov#wasInformedBy
prov:wasInformedBy rdf:type owl:ObjectProperty .


###  http://www.wikidata.org/prop/direct/P2888
<http://www.wikidata.org/prop/direct/P2888> rdf:type owl:ObjectProperty ;
                                            rdfs:subPropertyOf owl:sameAs ;
                                            rdf:type owl:SymmetricProperty ,
                                                     owl:TransitiveProperty ;
                                            rdfs:domain owl:Thing ;
                                            rdfs:range owl:Thing ;
                                            dc:description "(URLs only) used to link two items, indicating a high degree of confidence that the concepts can be used interchangeably"@en ,
                                                           "verwendet, um zwei Konzepte (Begriffe) zu verbinden, wobei mit großer Sicherheit gilt, dass beide untereinander auswechselbar sind"@de ;
                                            rdfs:label "exact match"@en ,
                                                       "exakte Übereinstimmung"@de .


